{
  "presets": [
    ["env", { "modules": false }],
    "stage-2",
    ["es2015", { "modules": false }]
  ],
  "plugins": [
  	"transform-runtime",
  	["component", [
	    {
	      "libraryName": "element-ui",
	      "styleLibraryName": "theme-chalk"
	    }
  	]]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}